<section class="section">
  <div class="container">
    <nav class="level">
      <div class="level-item has-text-centered">
        <div>
          <p class="heading">Wifi Speed</p>
          <p class="title"><%= venue.wifi_speed %></p>
        </div>
      </div>
      <div class="level-item has-text-centered">
        <div>
          <p class="heading">Price</p>
          <p class="title">
            <i class="fas fa-dollar-sign has-text-primary"></i>
            <i class="fas fa-dollar-sign has-text-primary"></i>
            <i class="fas fa-dollar-sign"></i>
          </p>
        </div>
      </div>
      <div class="level-item has-text-centered">
        <div>
          <p class="heading">Comfort</p>
          <p class="title">
            <%= raw venue.display_icons("comfort") %>
          </p>
        </div>
      </div>
      <div class="level-item has-text-centered">
        <div>
          <p class="heading">Plug sockets</p>
          <p class="title">
            <%= raw venue.display_icons("plug_sockets") %>
          </p>
        </div>
      </div>
      <div class="level-item has-text-centered">
        <div>
          <p class="heading">Quietness</p>
          <p class="title">
            <%= raw venue.display_icons("quietness") %>
          </p>
        </div>
      </div>
    </nav>
  </div>
</section>