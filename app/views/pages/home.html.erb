<div class="fullscreen home-bg">
  <div class="search-container">
    <h1 id="headline">wifiwarriors</h1>
    <p id="tagline">Remote working. On caffeine.</p>
    <div class="container">
      <%= form_with url: coffee_shops_path, local: true, method: :get, class: "form-group col-xs-6" do |form| %>
        <div class="body" align="center">
          <%= form.text_field :address, id: "autoComplete" %>
        </div>
        <!-- <%= form.submit "Search", class: "btn btn-primary" %> -->
      <% end %>

      <ul id="test-list"></ul>
    </div>
  </div>
</div>
<!-- autoComplete.min.js script -->
<script src="https://cdn.jsdelivr.net/npm/@tarekraafat/autocomplete.js@7.1.3/dist/js/autoComplete.min.js"></script>
